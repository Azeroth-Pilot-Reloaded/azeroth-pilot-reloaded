{
    "Lua.workspace.ignoreDir": [".vscode", "libs", "Scripts"],
    "git.allowForcePush": true,
    "Lua.diagnostics.disable": [
        "undefined-field",
        "redundant-parameter",
        "missing-parameter",
        "param-type-mismatch",
        "cast-local-type",
        "undefined-global"
    ],
    "[lua]": {
        "editor.defaultFormatter": "sumneko.lua"
    },
    "Lua.diagnostics.globals": [],
    "notebook.stickyScroll.enabled": true,
    "Lua.runtime.version": "Lua 5.1",
    "Lua.runtime.builtin": {
        "basic": "disable",
        "debug": "disable",
        "io": "disable",
        "math": "disable",
        "os": "disable",
        "package": "disable",
        "string": "disable",
        "table": "disable",
        "utf8": "disable"
    },
    "Lua.workspace.library": [
        "$USERPROFILE\\.vscode\\extensions\\ketho.wow-api-0.15.9\\Annotations"
    ]
}
